        <ff:CachedImage LoadingPlaceholder="logo.png" ErrorPlaceholder="close.png" Source="barcode.png" x:Name="ImageSource" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand" Aspect="AspectFit"/>
